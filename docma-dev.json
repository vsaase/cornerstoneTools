{
  "src": ["./src/**/*.js", "./README.md"],
  // TODO: Split at.. Each lib namespace, and top level
  // "src": [{ "Annotation": "./src/tools/annotation/*.js" }],
  "dest": "./docs/latest/api",
  "app": {
    "title": "Cornerstone Tools: API Docs",
    "routing": "query",
    "base": "/",
    "server": "github"
  },
  "jsdoc": {
    "hierarchy": true,
    "package": true,
    "private": false,
    "undescribed": false
  },
  "template": {
    "path": "zebra",
    "options": {
      "navbar": true,
      "sidebar": {
        "enabled": true,
        "outline": "tree"
      }
    }
  }
}
